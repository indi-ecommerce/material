            "json",
            "jsx",
            "ts",
            "tsx",
            "node",
            "scss"
        ]
    },
    "keywords": [
        "INDI",
        "Material",
        "Design"
    ],
    "dependencies": {
        "material-components-web": "^10.0.0",
        "sass": "^1.32.8"
    },
    "devDependencies": {
        "glob": "^7.1.6",
        "jest": "^26.6.3",
        "sass-true": "^6.0.1"
    }
