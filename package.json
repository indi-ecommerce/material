{
    "name": "@indi/material",
    "version": "0.0.3",
    "repository": "github:indi-ecommerce/material",
    "description": "INDI-Material resources",
    "author": "Marko Postma <marko.postma@indi.nl>",
    "license": "MIT",
    "sideEffects": false,
    "scripts": {
        "test": "jest test/scss.spec.js",
        "build": "sass theme/_index.scss dist/indi-material.css -I node_modules",
        "build:min": "sass theme/_index.scss dist/indi-material.min.css -I node_modules -s compressed"
